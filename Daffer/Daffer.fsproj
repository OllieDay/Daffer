<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Version>1.0.3</Version>
    <Authors>Ollie Day</Authors>
    <Title>Daffer</Title>
    <Description>Functional wrapper for Dapper.</Description>
    <Copyright>Copyright 2018, 2022 (c) Ollie Day</Copyright>
    <PackageTags>F#;Dapper;SQL</PackageTags>
    <PackageLicenseUrl>https://github.com/OllieDay/Daffer/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/OllieDay/Daffer</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="Daffer.fs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Dapper" Version="2.0.123" />
  </ItemGroup>

</Project>
